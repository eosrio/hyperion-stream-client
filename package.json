{
  "name": "@eosrio/hyperion-stream-client",
  "version": "1.0.0",
  "description": "Streaming Client for Hyperion History",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "test": "node examples/nodejs-demo.js",
    "debug": "cross-env DEBUG=* node examples/nodejs-demo.js",
    "build:esm": "tsc",
    "build:esm:watch": "tsc --watch",
    "build:cjs": "tsc --p tsconfig.commonjs.json",
    "build:webpack": "webpack",
    "build:webpack:watch": "webpack --watch",
    "build:all": "npm run build:esm && npm run build:cjs && npm run fix-hybrid-packages",
    "fix-hybrid-packages": "node scripts/create-dist-packages.mjs",
    "test:esm:debug": "node examples/esm_import.mjs --inspect"
  },
  "repository": "eosrio/hyperion-stream-client",
  "author": {
    "name": "EOS Rio",
    "url": "https://eosrio.io"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib/",
    "wrapper.mjs"
  ],
  "license": "MIT",
  "dependencies": {
    "async": "^3.2.4",
    "cross-fetch": "^3.1.5",
    "socket.io-client": "^4.5.2"
  },
  "devDependencies": {
    "@types/async": "^3.2.15",
    "@types/node": "^18.8.5",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.4.1",
    "typescript": "4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "bugs": {
    "url": "https://github.com/eosrio/hyperion-stream-client/issues"
  },
  "homepage": "https://github.com/eosrio/hyperion-stream-client#readme"
}
